{
    "name": "unicaen/sygal",
    "description": "Application de gestion des soutenances de th√®se",
    "repositories": [
        {
            "type": "composer",
            "url":  "https://gest.unicaen.fr/packagist"
        }
    ],
    "require-dev": {
        "unicaen/test":                         "^1.0",
        "zendframework/zend-developer-tools":   "^1.0"
    },
    "require": {
        "ext-fileinfo":                         "*",
        "ext-json":                             "*",
        "ext-imagick":                          "*",
        "unicaen/app":                          "^1.3",
        "unicaen/auth":                         "^1.2",
        "unicaen/faq":                          "^1.0",
        "unicaen/ldap":                         "^2.1",
