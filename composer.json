        "unicaen/leocarte":                     "^1.0",
        "unicaen/code":                         "^1.0",
        "ramsey/uuid":                          "^3.3",
        "mpdf/mpdf":                            "v6.1.3",
        "bjyoungblood/bjy-authorize":           "dev-master",
        "guzzlehttp/guzzle":                    "^6.3",
        "webmozart/assert":                     "^1.3",
        "beberlei/assert":                      "^2.9",
        "doctrine/migrations":                  "^1.5",
        "unicaen/oracle":                       "^1.2"
    },
    "autoload": {
        "psr-0": {
            "Application\\": "module/Application/src",
