    <field name="codeIso" type="string" column="code_iso" nullable="false"/>
    <field name="codeIsoAlpha3" type="string" column="code_iso_alpha3" nullable="false"/>
    <field name="codeIsoAlpha2" type="string" column="code_iso_alpha2" nullable="false"/>
    <field name="libelle" type="string" column="libelle" nullable="false"/>
    <field name="libelleIso" type="string" column="libelle_iso" nullable="false"/>
    <field name="libelleNationalite" type="string" column="libelle_nationalite" nullable="false"/>
    <field name="codePaysApogee" type="string" column="code_pays_apogee" nullable="true"/>
  </entity>
</doctrine-mapping>
