<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Application\Entity\Db\ImportObservEtabResult" table="IMPORT_OBSERV_ETAB_RESULT"
            repository-class="Application\Entity\Db\Repository\ImportObservEtabResultRepository">

        <id name="id" type="integer" column="ID"/>

        <field name="dateCreation" type="datetime" column="DATE_CREATION"/>
        <field name="sourceCode" column="SOURCE_CODE"/>
        <field name="resultat" column="RESULTAT"/>
        <field name="dateNotif" type="datetime" column="DATE_NOTIF" nullable="true"/>
        <field name="tooOld" type="boolean" column="TOO_OLD"/>

        <many-to-one field="importObservEtab" target-entity="Application\Entity\Db\ImportObservEtab">
            <join-columns>
                <join-column name="IMPORT_OBSERV_ETAB_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>

    </entity>
</doctrine-mapping>
