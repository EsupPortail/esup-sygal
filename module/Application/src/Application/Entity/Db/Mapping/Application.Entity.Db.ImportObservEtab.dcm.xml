<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Application\Entity\Db\ImportObservEtab" table="IMPORT_OBSERV_ETAB">

        <id name="id" type="integer" column="ID"/>

        <field name="enabled" type="boolean" column="ENABLED"/>

        <many-to-one field="importObserv" target-entity="Application\Entity\Db\ImportObserv">
            <join-columns>
                <join-column name="IMPORT_OBSERV_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="etablissement" target-entity="Application\Entity\Db\Etablissement">
            <join-columns>
                <join-column name="ETABLISSEMENT_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>

    </entity>
</doctrine-mapping>
