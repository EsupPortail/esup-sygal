<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Soutenance\Entity\Evenement" table="SOUTENANCE_EVENEMENT">

        <id name="id" type="integer" column="ID">
            <generator strategy="SEQUENCE"/>
        </id>

        <many-to-one field="proposition" target-entity="Soutenance\Entity\Proposition">
            <join-columns>
                <join-column name="PROPOSITION_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <field name="type" type="integer" column="TYPE_ID" nullable="false"/>
        <field name="date" type="datetime" column="DATE_EVENT" nullable="false"/>

    </entity>
</doctrine-mapping>
